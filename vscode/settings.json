{
	"[handlebars]": { "files.trimTrailingWhitespace": false },
	"[html]": { "editor.defaultFormatter": "vscode.html-language-features" },
	"[javascript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
	"[json]": { "editor.defaultFormatter": "vscode.json-language-features" },
	"[markdown]": { "files.trimTrailingWhitespace": false },
	"breadcrumbs.enabled": false,
	"editor.codeActionsOnSave": { "source.fixAll": true },
	"editor.cursorStyle": "line-thin",
	"editor.detectIndentation": false,
	"editor.foldingHighlight": false,
	"editor.fontFamily": "SF Mono, Roboto Mono, Consolas",
	"editor.fontLigatures": true,
	"editor.fontSize": 13,
	"editor.fontWeight": "300",
	"editor.insertSpaces": false,
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "all",
	"editor.scrollBeyondLastLine": false,
	"eslint.alwaysShowStatus": true,
	"eslint.format.enable": true,
	"eslint.workingDirectories": [{ "mode": "auto" }],
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.decorations.colors": false,
	"extensions.ignoreRecommendations": false,
	"files.trimFinalNewlines": false,
	"files.trimTrailingWhitespace": true,
	"git.enabled": false,
	"git.ignoreMissingGitWarning": true,
	"go.useLanguageServer": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	//"sonarlint.ls.javaHome": "/Library/Java/JavaVirtualMachines/jdk1.8.0_77.jdk/Contents/Home",
	"sonarlint.ls.javaHome": "C:\\Program Files\\Java\\jdk-17",
	"terminal.integrated.defaultProfile.windows": "Ubuntu-20.04 (WSL)",
	"terminal.integrated.fontFamily": "SF Mono, Roboto Mono, Consolas",
	"terminal.integrated.fontSize": 13,
	"terminal.integrated.fontWeight": "300",
	"terminal.integrated.fontWeightBold": "500",
	"terminal.integrated.lineHeight": 1.3,
	"workbench.activityBar.visible": true,
	"workbench.panel.defaultLocation": "right",
	"workbench.sideBar.location": "left",
	"workbench.startupEditor": "none",
	"workbench.tree.indent": 12,
	"workbench.tree.renderIndentGuides": "none",
}
