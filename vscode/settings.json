{
	"[css]": { "editor.defaultFormatter": "vscode.css-language-features" },
	"[handlebars]": { "files.trimTrailingWhitespace": false },
	"[html]": { "editor.defaultFormatter": "vscode.html-language-features" },
	"[java]": { "editor.defaultFormatter": "redhat.java" },
	"[javascript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
	"[json]": { "editor.defaultFormatter": "vscode.json-language-features" },
	"[markdown]": { "files.trimTrailingWhitespace": false },
	"breadcrumbs.enabled": false,
	"editor.codeActionsOnSave": { "source.fixAll": true },
	"editor.cursorStyle": "line-thin",
	"editor.detectIndentation": false,
	"editor.foldingHighlight": false,
	"editor.fontFamily": "SF Mono, Consolas",
	"editor.fontLigatures": true,
	"editor.fontSize": 13,
	"editor.fontWeight": "300",
	"editor.inlayHints.enabled": "off",
	"editor.insertSpaces": false,
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "all",
	"editor.scrollBeyondLastLine": false,
	"eslint.alwaysShowStatus": true,
	"eslint.format.enable": true,
	"eslint.workingDirectories": [{ "mode": "auto" }],
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.decorations.colors": false,
	"extensions.ignoreRecommendations": false,
	"files.trimFinalNewlines": false,
	"files.trimTrailingWhitespace": true,
	"git.enabled": false,
	"git.ignoreMissingGitWarning": true,
	"html.format.wrapLineLength": 0,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"redhat.telemetry.enabled": false,
	"security.workspace.trust.untrustedFiles": "open",
	//"sonarlint.ls.javaHome": "/Library/Java/JavaVirtualMachines/jdk-19.0.1.jdk/Contents/Home",
	"sonarlint.ls.javaHome": "C:\\Program Files\\Java\\jdk-19.0.1",
	"terminal.integrated.cwd": "",
	"terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
	"terminal.integrated.enablePersistentSessions": false,
	"terminal.integrated.fontFamily": "SF Mono, Consolas",
	"terminal.integrated.fontSize": 13,
	"terminal.integrated.fontWeight": "300",
	"terminal.integrated.fontWeightBold": "500",
	"terminal.integrated.lineHeight": 1.3,
	"terminal.integrated.persistentSessionReviveProcess": "never",
	"workbench.activityBar.visible": true,
	"workbench.colorCustomizations": { "sideBar.background": "#202020" },
	"workbench.panel.defaultLocation": "right",
	"workbench.sideBar.location": "left",
	"workbench.startupEditor": "none",
	"workbench.tree.indent": 12,
	"workbench.tree.renderIndentGuides": "none",
	"java.cleanup.actionsOnSave": [
		"qualifyMembers",
		"qualifyStaticMembers",
		"addOverride",
		"addDeprecated",
		"stringConcatToTextBlock",
		"invertEquals"
	],
	"sonarlint.rules": {
		"java:S1948": {
			"level": "off"
		},
		"java:S3776": {
			"level": "off"
		},
		"java:S3358": {
			"level": "off"
		},
		"javascript:S3776": {
			"level": "off"
		}
	}
}
